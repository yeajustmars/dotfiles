
export PATH="/run/current-system/sw/bin:/opt/homebrew/bin:/usr/sbin:/usr/local/bin:$PATH"
export EDITOR="nvim"
export TERM="alacritty"

source ~/.config/env.sh
source ~/.config/format.sh
source ~/.config/aliases.sh

autoload -Uz compinit bashcompinit
compinit
bashcompinit

source "$ST_ROOT_DIR/st-cli/$ST_CLI_VERSION/autocomplete.bash"

eval "$(starship init zsh)"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/mars/bin/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/mars/bin/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/mars/bin/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/mars/bin/google-cloud-sdk/completion.zsh.inc'; fi
